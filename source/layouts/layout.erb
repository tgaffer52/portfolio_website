        <link
      href="https://fonts.googleapis.com/css2?family=Major+Mono+Display&family=Montserrat&family=Raleway&display=swap"
      rel="stylesheet"
    >
          <link rel="icon" type="image/png" href="/images/logo.png">
          <script src="https://kit.fontawesome.com/f70b28dbbd.js" crossorigin="anonymous"></script>
        </head>
        <body>
          <%= partial "navbar" %>
          <div class="content">
            <%= yield %>
          </div>
          <%= partial "footer" %>
          <%= javascript_include_tag "site" %>
